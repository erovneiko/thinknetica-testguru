<%= form_with model: model, method: method, local: true do |form| %>
  <%= render 'shared/errors', object: @question %>

  <b><%= question_header(@question) %></b>

  <br><br>

  <%= form.label :text, 'Question:' %>&nbsp;
  <%= form.text_field :text, size: 60 %>

  <br><br>

  <%= form.submit %>
<% end %>
